<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>indexcontroller</servlet-name>
        <servlet-class>controlador.indexcontroller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LlenaSelectsLanding</servlet-name>
        <servlet-class>controlador.ServletsAdmin.LlenaSelectsLanding</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConsultarPedidosCliente</servlet-name>
        <servlet-class>controlador.ServletsAdmin.ConsultarPedidosCliente</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddCart</servlet-name>
        <servlet-class>controlador.AddCart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logincontroller</servlet-name>
        <servlet-class>controlador.logincontroller</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>indexcontroller</servlet-name>
        <url-pattern>/indexcontroller</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LlenaSelectsLanding</servlet-name>
        <url-pattern>/LlenaSelectsLanding</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConsultarPedidosCliente</servlet-name>
        <url-pattern>/ConsultarPedidosCliente</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddCart</servlet-name>
        <url-pattern>/AddCart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logincontroller</servlet-name>
        <url-pattern>/logincontroller</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>indexcontroller</welcome-file>
    </welcome-file-list>
</web-app>
